@model List<Cliente>
@{
    ViewData["Title"] = "Clientes Cedula Apellido";
}

<div class="contairner">
    <h3>
        Lista de Clientes por Cedula y Apellido
    </h3>
</div>


<div class="contairner">
    <div class="row">
        @if (Model.Count() > 0)
        {
            <table class="table table-bordered">
                <thead>
                    <tr>
                        <td>Apellidos</td>
                        <td>Cedula</td>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>                           
                            <td>@item.Apellidos</td>
                            <td>@item.Cedula</td>                          
                        </tr>
                    }
                </tbody>
            </table>
        }
        else
        {
            <p>No existe registros</p>
        }
    </div>
</div>